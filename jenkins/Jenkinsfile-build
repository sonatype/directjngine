library('private-pipeline-library')
library('jenkins-shared@add-feature-build-integration-testing-capability')

mavenSnapshotPipeline(usePublicSettingsXmlFile: true, disableLicenseCheck: true, integrationTests: true,
    onSuccess: { build, env ->
      notifyChat(env: env, currentBuild: build, room: 'nexus')
    },
    onFailure: { build, env ->
      notifyChat(env: env, currentBuild: build, room: 'nexus')
    }
)
