library('private-pipeline-library')
library('jenkins-shared@adding-sonar')

mavenSnapshotPipeline(usePublicSettingsXmlFile: true,
    onSuccess: { build, env ->
      notifyChat(env: env, currentBuild: build, room: 'nexus')
    },
    onFailure: { build, env ->
      notifyChat(env: env, currentBuild: build, room: 'nexus')
    }
)
