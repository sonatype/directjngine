library('private-pipeline-library')
library('jenkins-shared@add-license-check-disabling')

mavenSnapshotPipeline(usePublicSettingsXmlFile: true, disableLicenseCheck: true,
    onSuccess: { build, env ->
      notifyChat(env: env, currentBuild: build, room: 'nexus')
    },
    onFailure: { build, env ->
      notifyChat(env: env, currentBuild: build, room: 'nexus')
    }
)
