library('private-pipeline-library')
library('jenkins-shared')

mavenSnapshotPipeline(usePublicSettingsXmlFile: true, integrationTests: true, mavenOptions: '-Dlicense.skip=true',
    onSuccess: { build, env ->
      notifyChat(env: env, currentBuild: build, room: 'nexus')
    },
    onFailure: { build, env ->
      notifyChat(env: env, currentBuild: build, room: 'nexus')
    }
)
